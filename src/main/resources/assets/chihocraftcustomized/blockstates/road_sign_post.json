{
    "multipart": [
        {   "apply": { "model": "chihocraftcustomized:road_sign_post1" }},
        {   "when": { "north": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_post3", "uvlock": true }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "chihocraftcustomized:road_sign_post2", "uvlock": true }
        }
    ]
}
